            <property label="Object API Name" name="objectApiName" type="String"></property>
            <property label="SLDS Icon Name" name="sldsIconName" type="String"></property>
            <property label="Main Field" name="mainFieldApiName" type="String"></property>
            <property label="Secondary Field" name="secondaryFieldApiName" type="String"></property>
            <property label="Value Field" name="valueFieldApiName" type="String"></property>
            <property label="Max Rows" name="nRows" type="Integer"></property>
