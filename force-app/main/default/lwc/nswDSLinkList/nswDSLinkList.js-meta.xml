<?xml version="1.0" encoding="UTF-8"?>
<LightningComponentBundle xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>51.0</apiVersion>
    <isExposed>true</isExposed>
    <masterLabel>NSW DS Link List</masterLabel>
    <description>A link list in the NSW Government Design System</description>
    <targets>
        <target>lightningCommunity__Page</target>
        <target>lightningCommunity__Default</target>
        <target>lightning__FlowScreen</target>
    </targets>
    <targetConfigs>
        <targetConfig targets="lightningCommunity__Default">
            <property label="Links" name="links" default="[Link list](#)"
                type="String" description="Links in Markdown format"></property>
            <property label="NSW Class" name="nswClass"
                type="String" description="Additional styling to apply"></property>
        </targetConfig>
        <targetConfig targets="lightning__FlowScreen">
            <property label="Links" name="links" role="inputOnly"
                type="String" description="Links in Markdown format"></property>
            <property label="NSW Class" name="nswClass" role="inputOnly"
                type="String" description="Additional styling to apply"></property>
        </targetConfig>
    </targetConfigs>
</LightningComponentBundle>