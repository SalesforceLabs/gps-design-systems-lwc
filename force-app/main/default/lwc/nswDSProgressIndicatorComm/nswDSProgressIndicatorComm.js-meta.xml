<?xml version="1.0" encoding="UTF-8"?>
<LightningComponentBundle xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>52.0</apiVersion>
    <isExposed>true</isExposed>
    <masterLabel>NSW DS Progress Indicator</masterLabel>
    <description>A progress indicator in the NSW Government Design System</description>
    <targets>
        <target>lightningCommunity__Page</target>
        <target>lightningCommunity__Default</target>
    </targets>
    <targetConfigs>
        <targetConfig targets="lightningCommunity__Default">
            <property label="Current Step" name="currentStep" type="Integer"></property>
            <property label="Total Steps" name="totalSteps" type="Integer"></property>
            <property label="NSW Class" name="nswClass" type="String"></property>
        </targetConfig>
    </targetConfigs>
</LightningComponentBundle>
